<?xml version='1.1' encoding='UTF-8'?>
<hudson.plugins.createjobadvanced.CreateJobAdvancedPlugin plugin="createjobadvanced@1.9.1-SNAPSHOT">
  <autoOwnerRights>true</autoOwnerRights>
  <autoPublicBrowse>true</autoPublicBrowse>
  <replaceSpace>true</replaceSpace>
  <activeLogRotator>true</activeLogRotator>
  <daysToKeep>1</daysToKeep>
  <numToKeep>3</numToKeep>
  <artifactDaysToKeep>3</artifactDaysToKeep>
  <artifactNumToKeep>7</artifactNumToKeep>
  <activeDynamicPermissions>true</activeDynamicPermissions>
  <extractPattern>.*</extractPattern>
  <mvnArchivingDisabled>true</mvnArchivingDisabled>
  <mvnPerModuleEmail>true</mvnPerModuleEmail>
  <dynamicPermissionConfigs>
    <hudson.plugins.createjobadvanced.DynamicPermissionConfig>
      <groupFormat>authenticated</groupFormat>
      <checkedPermissionIds>
        <string>hudson.model.Item.Cancel</string>
        <string>hudson.model.Item.Delete</string>
        <string>hudson.model.Item.Read</string>
        <string>hudson.model.Item.Create</string>
        <string>hudson.model.Run.Delete</string>
        <string>hudson.model.Item.Workspace</string>
        <string>hudson.model.Item.Build</string>
        <string>hudson.model.Run.Replay</string>
        <string>hudson.model.Item.Move</string>
        <string>hudson.model.Item.Configure</string>
        <string>hudson.model.Item.Discover</string>
        <string>hudson.model.Run.Update</string>
      </checkedPermissionIds>
    </hudson.plugins.createjobadvanced.DynamicPermissionConfig>
  </dynamicPermissionConfigs>
</hudson.plugins.createjobadvanced.CreateJobAdvancedPlugin>
